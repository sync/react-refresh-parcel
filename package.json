{
  "name": "@parcel/react-refresh-example",
  "version": "2.0.0-alpha.3.2",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "concurrently 'parcel src/index.html' 'yarn relay-watch'",
    "build": "parcel build src/index.html",
    "clean": "rm -rf .parcel-cache dist",
    "relay": "relay-compiler",
    "relay-watch": "relay-compiler --watch",
    "lint": "eslint '**/*.js{,x}'",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md,html,graphql}\""
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@parcel/babel-preset": "2.0.0-alpha.2",
    "autoprefixer": "9.7.3",
    "babel-plugin-relay": "7.0.0",
    "concurrently": "5.0.1",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.17.0",
    "graphql": "14.5.8",
    "parcel": "2.0.0-alpha.3.2",
    "postcss-modules": "1.4.1",
    "prettier": "1.19.1",
    "relay-compiler": "7.0.0",
    "relay-config": "7.0.0",
    "tailwindcss": "1.1.4"
  },
  "dependencies": {
    "isomorphic-fetch": "2.2.1",
    "onegraph-auth": "2.1.1",
    "react": "0.0.0-experimental-b53ea6ca0",
    "react-dom": "0.0.0-experimental-b53ea6ca0",
    "react-relay": "0.0.0-experimental-a1a40b68",
    "relay-runtime": "7.0.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true
  }
}
